<% include ./partials/header %>
 <link rel="stylesheet" type="text/css" href="/contactpage.css">


<div class="container">
  <div class="contact-section">
      
           
      
    <h2 class="ct-section-head">CONTACT US</h2>
 
    <div style="margin-top:30px;">
    <h4>Whether if it's questions about SellText, a midlife crisis, or stress from school, feel free to contact us about anything.</h4>
    <h4>Constructive criticism is always welcome.</h4>
    </div>
    
    <div class="row contact-fields">
      
      <div class="col-md-8 left-form">
           
          <form class="gform" method="POST"  action="https://script.google.com/macros/s/AKfycbzyiGU7WvK-FJ3A25kVZZidpCAtpbRz7gDqqcpk/exec">
              <div style="display:none;" class="thankyou_message alert alert-success" role="alert">
         <!-- You can customize the thankyou message by editing the code below -->
         
         <p class="contactFlash" style=" text-align:center;">Thanks for contacting us!</p>
         <p class="contactFlash" style="text-align:center;">We will get back to you soon</p>
        </div>
               
        <div class="form-group">
          <label class="sr-only" for="fname">First Name *</label>
          <input class="required form-control" id="fname" name="fname" placeholder="First Name&nbsp;*" type="text"  required>
        </div>
        <div class="form-group">
          <label class="sr-only" for="lname">Last Name *</label>
          <input class="required form-control" id="lname" name="lname" placeholder="Last Name&nbsp;" type="text">
        </div>
        <div class="form-group">
          <label class="sr-only" for="contactEmail">Email *</label>
          <input class="required form-control h5-email" id="contactEmail" name="contactEmail" placeholder="Email&nbsp;*" type="text"  required>
        </div>
        <div class="form-group">
          <label class="sr-only" for="comment">Type your message here</label>
          <textarea class="required form-control" id="comment" name="comment" placeholder="Type your message here&nbsp;*" rows="6" maxlength="5000" required></textarea>
        </div>
        <button class="btn btn-accent" type="submit">Submit</button>  
        
        <div style="display:inline; display:none; margin-left: 40px;" class="thankyou_check">
         <!-- You can customize the thankyou message by editing the code below -->
        <i style="color:#21CA3D; font-size: 25px;" class="fas fa-check"></i>
        </div>
        
      </form>
    </div>
    <div class="col-md-4 contact-info">
      <div class="email">
        <h2>Email</h2>
        <a style="color: blue" href="mailto:admin@selltext.org">admin@selltext.org</a>
      </div>
      <div class="email">
        <h2>Social</h2>
        <a style="" href="http://instagram.com/_u/selltext_/"><i style="font-size:40px;" class="fab fa-instagram"></i></a>
   <a style="margin-left:20px;" href="https://twitter.com/SellText"><i style="font-size:40px;" class="fab fa-twitter"></i></a>
      <a style="margin-left:20px;" href="mailto:admin@selltext.org"><i style="font-size:40px;" class="fas fa-envelope"></i></a>
      </div>
    </div>
  </div>
</div>


<script data-cfasync="false" type="text/javascript" src="form-submission-handler.js"></script>

<% include ./partials/footer %>